<!-- PP Registrion JS Code -->
<script type="text/javascript">
  
  $(document).ready(function() {
  
  
  	/* Formatting */
  	window.resizeTo(600, 800);
  
    
	/* System Check */
	var genreid 		= $('#genreid').val();
	var movieid 		= $('#movieid').val();
	var redirect_url 	= $('#redirect_url').val();
	var xaction 		= $('#xaction').val();

	if(!genreid){
	  $('#pp_message_box').append('<div class="pp_error">GENRE ID IS MISSING!</div>');
	}
	if(!movieid){
	  $('#pp_message_box').append('<div class="pp_error">MOVIE ID IS MISSING!</div>');
	}
	if(!redirect_url){
	  $('#pp_message_box').append('<div class="pp_error">REDIRECT URL IS MISSING!</div>');
	}
	if(xaction != 'RemoteSubmit'){
	  $('#pp_message_box').append('<div class="pp_error">XACTION MUST BE \'RemoteSubmit\'</div>');
	}
	if(googleAnalyticsTrackingID == 'UA-XXXXXXXX-XXX' || googleAnalyticsTrackingID == ''){
	  $('#pp_message_box').append('<div class="pp_error">GOOGLE TRACKING (UA Number) IS MISSING!</div>');
	}
	
	//Check For Under Age User Cookie
	if($.cookie('email_registration_agegate')){
		_gaq.push(['_trackEvent', 'agegate', 'failed', 'Failed JS General Min. Age']);
		window.location.href = "sorry.html";
	}
		
	//Start jQuery Form Validation
	$("#SubscribeForm").validationEngine({
					scroll: false,
					onFailure: function(form, status){
   								 $('#pp_clear').show();
 				 				}
	});
	
	//Hide Tooltips
	$("#pp_clear").click(function() {
		$('#SubscribeForm').validationEngine('hide');
		$('#pp_clear').hide();
	});
			
	//Phone Validation (Allow Only Numbers)
    $("#mobile_phone").keydown(function(event) {
        // Allow: backspace, delete, tab and escape
        if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || 
             // Allow: Ctrl+A
            (event.keyCode == 65 && event.ctrlKey === true) || 
             // Allow: home, end, left, right
            (event.keyCode >= 35 && event.keyCode <= 39)) {
                 // let it happen, don't do anything
                 return;
        }
        else {
            // Ensure that it is a number and stop the keypress
            if ((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
                event.preventDefault(); 
            }   
        }
    });
	
	$('#SubscribeForm').submit(function() {
		
		function checkAge(){

			var min_age = 17;
		
			var form = document.forms["SubscribeForm"];
			var month = (parseInt(form.month.options[form.month.selectedIndex].value)-1);
			var day = parseInt(form.day.options[form.day.selectedIndex].value);
			var year = parseInt(form.year.options[form.year.selectedIndex].value);
		
			var user = new Date((year+min_age), month, day);
			var today = new Date();
		
			if(today.getTime() - user.getTime() < 0) {
				return false;
			
			} else {
				return true;
			}
		}
		
		if((document.SubscribeForm.month.selectedIndex != 0)&&
		   (document.SubscribeForm.day.selectedIndex != 0)&&
		   (document.SubscribeForm.year.selectedIndex != 0)){
			if(!checkAge()){
				_gaq.push(['_trackEvent', 'agegate', 'failed', 'Failed JS General Min. Age']);
				
				//Set Session Cookie
				$.cookie('email_registration_agegate', 'User Under 13');
				
				window.location.href = "sorry.html";
				return false;
			}
		}
				
		return true;
	}); 	
  });
</script>
